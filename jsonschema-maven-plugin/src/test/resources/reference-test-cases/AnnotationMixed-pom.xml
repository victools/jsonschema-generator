<project>
  <build>
    <plugins>
      <plugin>
        <groupId>com.github.victools</groupId>
        <artifactId>jsonschema-maven-plugin</artifactId>
        <configuration>
          <annotations>
            <annotation>com.github.victools.jsonschema.plugin.maven.annotations.TestAnnotation</annotation>
            <annotation>com.github.victools.jsonschema.plugin.maven.annotations.AnotherTestAnnotation</annotation>
          </annotations>
          <classNames>
            <className>com.github.victools.jsonschema.plugin.maven.testpackage.TestClassA</className>
          </classNames>
          <schemaFilePath>target/generated-test-sources/AnnotationMixed</schemaFilePath>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
