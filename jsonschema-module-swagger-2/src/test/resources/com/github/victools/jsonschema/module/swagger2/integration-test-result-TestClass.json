{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "fieldWithDescriptionAndAllowableValues": {
            "type": ["string", "null"],
            "description": "field description",
            "enum": ["A", "B", "C", "D"],
            "minLength": 1,
            "maxLength": 1
        },
        "fieldWithExclusiveNumericRange": {
            "type": "integer",
            "exclusiveMinimum": 14,
            "exclusiveMaximum": 21,
            "multipleOf": 0.1
        },
        "fieldWithInclusiveNumericRange": {
            "type": "number",
            "minimum": 15,
            "maximum": 20,
            "multipleOf": 0.0123456789
        },
        "fieldWithOverriddenName": {
            "minItems": 1,
            "maxItems": 20,
            "type": "array",
            "items": {
                "type": ["boolean", "null"],
                "default": "true"
            }
        }
    },
    "required": ["fieldWithExclusiveNumericRange", "fieldWithOverriddenName"]
}
